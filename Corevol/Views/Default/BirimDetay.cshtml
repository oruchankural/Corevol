@using Corevol.Models
@model List<Personel>
@{
    ViewData["Title"] = "BirimDetay";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<br />

<div class="container-fluid">
    <h3>@ViewBag.birim</h3>
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Şehir</th>

        </tr>
        @foreach (var x in Model)
        {



            <tr>
                <td>@x.PersonelID</td>
                <td>@x.Ad</td>
                <td>@x.Soyad</td>
                <td>@x.Sehir</td>




            </tr>
        }

    </table>

</div>



